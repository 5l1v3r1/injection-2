<?xml version="1.0" encoding="UTF-8"?>
<objects path="lib/object">
  <!-- object with pre-defined members -->
  <object id="obj:foo2" mixin="mixin1,mixin2">
    <property name="abc" value="bcd" />
    <property name="num" value="1" type="number" />
  </object>
  <mixin id="mixin1">
    <property name="def">
      <object id="obj:bar" path="bar"></object>
    </property>
  </mixin>
  <mixin id="mixin2">
    <property name="hello" value="world"></property>
  </mixin>
</objects>
