<?xml version="1.0" encoding="UTF-8"?>

<objects path="lib/list">
  <!-- list with refs -->
  <object id="list:obj1" path="obj1">
    <property name="things">
      <list>
        <ref object="obj:foo"></ref>
        <ref object="obj:bar"></ref>
      </list>
    </property>
  </object>

  <!-- list with maps -->
  <object id="list:obj2" path="obj2">
    <property name="things">
      <list>
        <map>
          <entry key="foo" value="bar" />
          <entry key="foo">
            <object>
              <property name="a" value="b" />
            </object>
          </entry>
        </map>
        <map>
          <entry key="bar" value="foo" />
        </map>
      </list>
    </property>
  </object>

  <!-- list with sets -->
  <object id="list:obj3" path="obj3">
    <property name="things">
      <list>
        <set>
          <ref object="obj:foo"></ref>
          <ref object="obj:bar"></ref>
        </set>
      </list>
    </property>
  </object>

</objects>
