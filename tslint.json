{
  "linterOptions": {
    "exclude": [
      "**/test/**"
    ]
  },
  "extends": [
    "tslint-midway-contrib"
  ],
  "rules": {
    "no-angle-bracket-type-assertion": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "imports": "ignore"
        }
      }
    ]
  }
}
